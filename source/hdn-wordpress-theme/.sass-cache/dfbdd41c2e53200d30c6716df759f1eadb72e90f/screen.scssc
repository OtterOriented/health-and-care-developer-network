/* Welcome to Compass.
 * In this file you should write your main styles. (or centralize your imports)
 * Import this file using the following HTML or equivalent:
 * <link href="/stylesheets/screen.css" media="screen, projection" rel="stylesheet" type="text/css" /> */

@import "compass/reset";
@import "compass/css3";
@import "partials/reset";
@import "partials/variables";
@import "partials/mixins";
@import "partials/type";
@import "partials/layout";
@import "partials/modules";

@import "partials/ie";
 
/*------------------------------------*\
	$CLEARFIX
\*------------------------------------*/
/*
Big up @necolas: http://nicolasgallagher.com/micro-clearfix-hack/
*/
.cf:before,
.cf:after{
	content:"";
	display:table;
}
.cf:after{
	clear:both;
}
.cf{
	zoom:1;
}

.clear{
	clear:both;
}
/*----------------------------------------*/



body{
	padding-top: 46px;
}



body::before{
	content: "";
	width: 100%;
	height: 12px;
	position: absolute;
	top: 0;
	left: 0;
	right: 0;
	background-position: -100% 0;
}

.svg body::before{
	background: url(../img/css/lid_bg.svg) repeat-x;
	}

.no-svg body::before{
	background: url(../img/css/lid_bg.jpg) repeat-x;
}

.wrapper{
	max-width: 1024px;
	margin: 0px auto;
	position: relative;
	padding: 0 15px;
}

.no-svg .site-logo .logo{
	background: url(../img/css/logo.jpg) no-repeat;
}

.svg .site-logo .logo{
	background: url(../img/css/logo.svg) no-repeat;
}
.site-logo{
	display: block;
	width: 193px;
	height: 60px;
	float: left;
	
	.logo{
		width: 193px;
		height: 60px;
	}
}


.no-svg input.search_btn{
	background: url(../img/css/search_btn_06.jpg) no-repeat;
}

.svg input.search_btn{
	background: url(../img/css/search_btn.svg) no-repeat;
}


.beta_nav{
	float: right;

	.search_box{
		width: 245px;
		position: relative;
		float: right;
	}

	form{
		height: 33px;

		input.search_box{
			outline: none;
			color: #7c868d;
			font-size: 13px;
			font-size: 0.81rem;
			height: 17px;
			padding: 7px 1%;
			width: 98%;
			border: 1px solid rgba(180,180,180,.65);
			background-color: #fff;
			@include box-shadow(-1px 1px 0 rgba(255,255,255,.1), inset -1px 2px 3px rgba(0,0,0,.05));
		}

		input.search_btn{
			width: 40px;
			height: 33px;
			border: none;
			padding-top: 6px;
			position: absolute;
			right: 0;
			top: 0;
		}
	}

	ul{
		float: left;
		margin-top: 5px;
		li{
			@include horiz-nav;
			border-right: 1px solid $darkGrey;
			a{
				color: $darkGrey;
				padding: 0 20px;
				font-size: 14px;
				font-size: 0.88rem;
			}
		}
		li:last-child{
			border-right: 0;
		}	
	}
}

.svg .nhs_network .nhs{
	background: url(../img/css/nhs_logo.svg) no-repeat;
}

.no-svg .nhs_network .nhs{
	background: url(../img/css/nhs_logo.jpg) no-repeat;
}

.nhs_network{
	position: absolute;
	right: 10px;
	top: 50px;

	p{
		font-size: 12px;
		color: #6b6b6b;
	}
	.nhs{
		display: inline-block;
		position: relative;
		top: 3px;
		width: 45px;
		height: 18px;
		text-indent: -9999px;
		margin: 0 3px;
	}	
}	

.main_nav{
	background: darken($blue, 10%);
	height: 58px;
	margin-top: 40px;
	ul{
		max-width: 100%;
		height: 54px;
		border: 1px solid #008aef;
		background-color: #0071c5;
		@include box-shadow(0 0 4px rgba(0,0,0,.22));
		position: relative;
		bottom: 10px;
		li{
			@include horiz-nav;
			display: block;
			text-align: center;
			a{
				color: #fff;
				padding-top: 16px;
				height: 38px;
				display: block;
				border-right: 1px solid lighten($blue, 10%);
				-webkit-transition: all 0.3s ease-in-out;
				-moz-transition: all 0.3s ease-in-out;
				-o-transition: all 0.3s ease-in-out;
				-ms-transition: all 0.3s ease-in-out;
				transition: all 0.3s ease-in-out;
			}
			a:hover{
				text-decoration: none;
			};

		}
		li.library{
			width: 9.68%;
			a:hover{
				background: $green;
			};
		}
		li.downloads{
			width: 21.11%;
			a:hover{
				background: $red;
			};
		}
		li.test{
			width: 15.24%;
			a:hover{
				background: $purple;
			};
		}
		li.learn{
			width: 9.98%;
			a:hover{
				background: $orange;
			};
		}
		li.community{
			width: 13.59%;
			a:hover{
				background: $aquaGreen;
			};
		}
		li.market{
			width: 15.85%;
			a:hover{
				background: $aquaBlue;
			};
		}
		li.contributors{
			width: 14.52%;
			a:hover{
				background: lighten($blue, 5%);
			};
			a{
				border-right: 0;
			}
		}
	}

}

a.mob_nav_btn{
		display: none;
	}


.no-svg .page_title, .no-svg .hp_page_title{
	background: url(../img/css/header_bg_09.jpg);
}

.svg .page_title, .svg .hp_page_title{
	background: url(../img/css/header_bg.svg);
}

.page_title{
	hgroup{
		background: url(../img/css/library_header_icon.svg) no-repeat;
		background-position: left center;
		padding: 42px 0 42px 200px;
	}

	h1{
		color: #fff;
	}

	h2{
		color: #fff;
		font-size: 24px;
		font-size: 1.50rem;
		font-weight: 300;
	}
}

.hp_page_title{
	height: 255px;
	margin-bottom: 48px;

	h1{
		padding: 45px 0 10px 0;
		color: #fff;
	}

	h2{
		color: #fff;
		font-size: 24px;
		font-size: 1.50rem;
		font-weight: 300;
	}

	p{
		position: relative;
		top: 40px;
		color: #fff;
		.btn{
			margin-right: 10px;
			position: relative;
			bottom: 10px;
		}
	}
}


.breadcrumbs{
	height: 35px;
	padding: 13px 0 0 0;
	border-bottom: 1px solid $midGrey;
	font-size: 14px;
	font-size: 0.88rem;
	color: $metaGrey;
	p{
		float: left;
		margin-right: 10px;
	}
	ul{
		li{
			@include horiz-nav;
			.bc_arrow{
				font-size: 40px;
				line-height: 0px;
				position: relative;
				top: 6px;
				padding: 0 10px;
			}

		}
	}
	a{
		color: $metaGrey !important;
	}
}


a.btn{
	color: #fff;
	@include box-shadow(0 0 4px rgba(0,0,0,.3));
	padding: 10px 20px;
	display: block;
	float: left;
	-webkit-transition: all 0.3s ease-in-out;
	-moz-transition: all 0.3s ease-in-out;
	-o-transition: all 0.3s ease-in-out;
	-ms-transition: all 0.3s ease-in-out;
	transition: all 0.3s ease-in-out;
}

a.red_btn{
	border: 1px solid lighten($red, 10%);
	background-color: $red;
}
a.red_btn:hover{
	text-decoration: none;
	@include box-shadow(0 0 10px rgba(0,0,0,.5));
	background-color: lighten($red, 3%);
}

a.aquaGreen_btn{
	border: 1px solid lighten($aquaGreen, 10%);
	background-color: $aquaGreen;
}
a.aquaGreen_btn:hover{
	text-decoration: none;
	@include box-shadow(0 0 10px rgba(0,0,0,.5));
	background-color: lighten($aquaGreen, 3%);
}


[data-icon]:before {
	font-family: 'entypo'; /* BYO icon font, mapped smartly */
	content: attr(data-icon);
	speak: none; /* Not to be trusted, but hey. */
}

.btn_icon{
	position: relative;
	top: 4px;
	font-size: 40px;
	line-height: 0px;
}



.community_latest{
	margin: 0 15px 0 15px;
	h3{
		padding-bottom: 20px;
		border-bottom: 1px solid $midGrey;
		margin-bottom: 20px;
	}
	article{
		margin-bottom: 24px;
		.meta{
			font-size: 14px;
			font-size: 0.88rem;
			color: $metaGrey;
			margin-bottom: 10px;
		}
		a.read_more{
			display: block;
			font-size: 14px;
			margin-top: 10px;
		}
	}
}


.content_wrap{
	h2.underline{
		padding-bottom: 12px;
		border-bottom: 1px solid $midGrey;
		margin: 0 0 35px 30px;
	}

	h3{
		padding-bottom: 5px;
	}

	.link_arrow{
		line-height: 0px;
		font-size: 40px;
		margin-left: 8px;
	}

	a:hover h3 .link_arrow{
		text-decoration: none;
	}

	article{
		margin: 0 0 24px 30px;
		hgroup{
			margin-bottom: 24px;
			h2{
				margin-bottom: 0;
			}
			h3{
				color: $midGrey;
			}
		}
		h2{
			margin-bottom: 5px;
		}
		p{
			margin-bottom: 24px;
		}

		ul{
			list-style: disc;
			padding-left: 40px;
			margin-bottom: 24px;
		}
		ol{
			list-style: decimal;
			padding-left: 40px;
			margin-bottom: 24px;
		}
	}

	article.lib_list{
		padding-bottom: 24px;
		border-bottom: 1px solid $midGrey;
		margin: 0 0px 23px 30px;

	}

	ul.attachments{
		list-style: none;
		padding-left: 15px;
		margin:0 0 30px 30px;
		border-left: 2px solid $green; 
		li{
			margin-bottom: 10px;
			a{
				font-size: 14px;
				font-size: 0.88rem;
				color: $metaGrey;
			}
		}

		.att_icon{
			font-size: 35px;
			position: relative;
			top: 4px;
			padding-right: 5px;
			line-height: 0px;
		}
	}
}


footer{
	background: url(../img/css/header_bg_09.jpg);
	height: 96px;
	ul{
		float: left;
		margin-top: 35px;
		margin-left: -20px;
		li{
			@include horiz-nav;
			a{
				font-size: 14px;
				font-size: 0.88rem;
				color: #fff;
				padding: 0 20px;
				border-right: 1px solid #fff;
			}
		}
	}
	p{
		float: right;
		color: #fff;
		margin-top: 35px;
		font-size: 14px;
		font-size: 0.88rem;
	}
}

input {
    -webkit-appearance: none;
    border-radius: 0;
}

